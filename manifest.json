{
  "name": "Text Macros & LaTeX",
  "version": "1.3.11",
  "description": "Text macros and a LaTeX preset",
  "permissions": [
    "storage"
  ],
  "background": {
    "scripts": ["onInstalled.js"]
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "css": [],
      "js": [
        "macros.js",
        "presets.js",
        "runtime.js"
      ]
    }
  ],
  "options_page": "options.html",
  "manifest_version": 2,
  "icons": {
    "16": "icons/ico16.png",
    "48": "icons/ico48.png",
    "128": "icons/ico128.png"
  }
}
